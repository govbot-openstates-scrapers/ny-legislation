{"legislative_session": "2025-2026", "identifier": "S5916", "title": "Requires the commissioner of taxation and finance to provide for the payment of refunds within 30 days of the receipt of a tax return", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Requires the commissioner of taxation and finance to provide for the payment by the department of taxation and finance of tax refunds within 30 days of the receipt of a taxpayer's tax return, unless there is a discrepancy in such return, in which case the department shall provide written notice to the taxpayer of such discrepancy and a date when the discrepancy will be resolved; provides failure to comply with such refund and notice requirement shall make the department of taxation and finance liable for interest on the refund due at a rate of 6% per annum."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO INVESTIGATIONS AND GOVERNMENT OPERATIONS", "date": "2025-03-04", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "INVESTIGATIONS AND GOVERNMENT OPERATIONS", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Alexis Weik", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Alexis Weik\"}", "organization_id": null}], "related_bills": [{"identifier": "S 5243", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "S 5917", "legislative_session": "2021-2022", "relation_type": "prior-session"}, {"identifier": "S 4157", "legislative_session": "2019-2020", "relation_type": "prior-session"}, {"identifier": "S 2152", "legislative_session": "2017-2018", "relation_type": "prior-session"}, {"identifier": "S 1782", "legislative_session": "2015-2016", "relation_type": "prior-session"}, {"identifier": "S 3047", "legislative_session": "2013-2014", "relation_type": "prior-session"}, {"identifier": "S 2631", "legislative_session": "2011-2012", "relation_type": "prior-session"}, {"identifier": "S 3985", "legislative_session": "2009-2010", "relation_type": "prior-session"}, {"identifier": "S 3658", "legislative_session": "2007-2008", "relation_type": "prior-session"}], "versions": [{"note": "S5916", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=printbill&bn=S5916&term=2025&Text=Y", "media_type": "text/html"}, {"url": "http://legislation.nysenate.gov/pdf/bills/2025/S5916", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [{"publication": "New York Laws", "citation": "TAX \u00a7 171", "citation_type": "proposed", "effective": null, "expires": null, "url": "https://www.nysenate.gov/legislation/laws/TAX/"}], "sources": [{"url": "https://legislation.nysenate.gov/api/3/bills/2025-2026/S5916?summary=&detail=", "note": ""}, {"url": "http://www.nysenate.gov/legislation/bills/2025/S5916", "note": ""}, {"url": "https://nyassembly.gov/leg/?bn=S05916&term=2025", "note": ""}], "extras": {}, "jurisdiction": {"id": "ocd-jurisdiction/country:us/state:ny/government", "name": "New York", "classification": "state", "division_id": "ocd-division/country:us/state:ny"}, "scraped_at": "2025-12-30T08:17:57+00:00", "_id": "0c552576-e558-11f0-8578-5e03ffaca1f9"}