{"legislative_session": "2025-2026", "identifier": "A1771", "title": "Relates to school district state aid eligibility", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Provides that no school district shall be eligible to receive the full apportionment of state aid to which it is entitled if evidence is found by the state comptroller when an audit is conducted and such audit finds that a district has inappropriate excess of their unexpended surplus funds and such school district did not reduce the school tax levy in a proportion equal to the excess of four percent of the current year school budget."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO EDUCATION", "date": "2025-01-14", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "EDUCATION", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Michael J. Fitzpatrick", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Michael J. Fitzpatrick\"}", "organization_id": null}], "related_bills": [{"identifier": "A 4299", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "A 5610", "legislative_session": "2021-2022", "relation_type": "prior-session"}, {"identifier": "A 5371", "legislative_session": "2019-2020", "relation_type": "prior-session"}], "versions": [{"note": "A1771", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=printbill&bn=A1771&term=2025&Text=Y", "media_type": "text/html"}, {"url": "http://legislation.nysenate.gov/pdf/bills/2025/A1771", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "https://legislation.nysenate.gov/api/3/bills/2025-2026/A1771?summary=&detail=", "note": ""}, {"url": "http://www.nysenate.