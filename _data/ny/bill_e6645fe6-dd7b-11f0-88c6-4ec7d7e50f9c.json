{"legislative_session": "2025-2026", "identifier": "S1358", "title": "Proscribes persons, firms or corporations engaged in retail business of selling firearms from selling, delivering or transferring child operated firearms", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Prohibits persons, firms or corporations engaged in the retail business of selling firearms from selling, delivering or transferring child operated firearms; defines \"child operated firearm\" to mean a pistol or revolver manufactured 1 year after the effective date of these provisions which does not contain a childproofing device or mechanism incorporated into the design of such pistol or revolver to effectively preclude an average 5 year old from firing same; makes violations a class A misdemeanor."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO CODES", "date": "2025-01-09", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "CODES", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Jos\u00e9 M. Serrano", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Jos\\u00e9 M. Serrano\"}", "organization_id": null}, {"name": "Jamaal Bailey", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Jamaal Bailey\"}", "organization_id": null}, {"name": "Liz Krueger", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Liz Krueger\"}", "organization_id": null}, {"name": "James Sanders Jr.", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"James Sanders Jr.\"}", "organization_id": null}], "related_bills": [{"identifier": "S 803", "legislative_session": "2023-2024", "relatio