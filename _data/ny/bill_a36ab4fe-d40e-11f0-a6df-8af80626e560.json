{"legislative_session": "2025-2026", "identifier": "S8575", "title": "Relates to cannabinoid hemp beverages", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Enacts the \"hemp beverage and taxation act\"; defines cannabinoid hemp beverage; prohibits the sale of cannabinoid hemp beverages to persons twenty-one years of age or older; provides requirements for the product or labeling of such products; provides for offenses for persons under the age of twenty-one years who purchase or attempt to purchase a cannabinoid hemp beverage through fraudulent means and persons who procure such beverages for persons under the age of twenty-one years; imposes advertising requirements; imposes a tax on cannabinoid hemp products; provides for the disbursement of revenues collected pursuant to Article 20-e of the Tax Law."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO RULES", "date": "2025-11-12", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "RULES", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Jeremy Cooney", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Jeremy Cooney\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "S8575", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=printbill&bn=S8575&term=2025&Text=Y", "media_type": "text/html"}, {"url": "http://legislation.nysenate.gov/pdf/bills/2025/S8575", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [{"publication": "New York Laws", "citation": "TAX \u00a7 A20-E", "citation_type": "proposed", "effective": null, "expires": null, "url": "https://www.nysenate.gov/legislation/laws/TAX/"}, {"publication": "New York Laws", "citation": "CAN", "citation