{"legislative_session": "2025-2026", "identifier": "A4550", "title": "Requires the department of labor to study the long-term impact of artificial intelligence on the state workforce", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Requires the department of labor to study the long-term impact of artificial intelligence on the state workforce including but not limited to job performance, productivity, training, education requirements, privacy and security; prohibits any state entity from using artificial intelligence in any way that would result in the displacement of any currently employed worker or loss of position, including partial displacement such as a reduction in the hours of non-overtime work, wages or employment benefits, or results in the impairment of existing collective bargaining agreements."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO LABOR", "date": "2025-02-04", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "LABOR", "entity_type": "organization", "organization_id": null}]}, {"description": "AMEND (T) AND RECOMMIT TO LABOR", "date": "2025-04-18", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["amendment-passage", "referral-committee"], "related_entities": []}, {"description": "PRINT NUMBER 4550A", "date": "2025-04-18", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["amendment-passage"], "related_entities": []}, {"description": "REPORTED REFERRED TO WAYS AND MEANS", "date": "2025-04-30", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "WAYS AND MEANS", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Brian Cunningham", "classification": "primary", "entity_type": "perso