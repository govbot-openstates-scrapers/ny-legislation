{"legislative_session": "2025-2026", "identifier": "A4930", "title": "Relates to the creation of an adult high school workforce development demonstration project", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Creates an adult high school workforce development demonstration project in Rochester; defines \"adult high school workforce development demonstration project\" or \"program\" to mean a private or independent high school registered by the department which provides adult learners with an appropriate academic environment, career credentialing programs and access to wraparound services provided by the operator or any entity partnered which such operator to address barriers to enable adult learners to pursue and obtain, subject to satisfactory completion of the requirements set forth within this article, a high school diploma."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO EDUCATION", "date": "2025-02-10", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "EDUCATION", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Harry B. Bronson", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Harry B. Bronson\"}", "organization_id": null}, {"name": "William Colton", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"William Colton\"}", "organization_id": null}, {"name": "Demond Meeks", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Demond Meeks\"}", "organization_id": null}, {"name": "Sarah Clark", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Sarah Clark\"}", "organization_id": null}], "related_bills": [{"identifier":