{"legislative_session": "2025-2026", "identifier": "A2543", "title": "Requires persons or entities providing benefits in the event of disability to not impose onerous or unreasonable conditions for the payment of benefits thereunder", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Requires persons or entities providing benefits in the event of disability to not impose \"onerous or unreasonable conditions\" for the payment or continued payment of benefits thereunder, and provides for the enforcement thereof including actual damages, interest and penalties, reasonable costs and attorney's fees, and in the court's discretion, exemplary damages; exempts worker's compensation and NYS disability insurance."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO JUDICIARY", "date": "2025-01-17", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "JUDICIARY", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Jeffrey Dinowitz", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Jeffrey Dinowitz\"}", "organization_id": null}, {"name": "David Weprin", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"David Weprin\"}", "organization_id": null}, {"name": "Rebecca Seawright", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Rebecca Seawright\"}", "organization_id": null}], "related_bills": [{"identifier": "A 1797", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "A 2191", "legislative_session": "2021-2022", "relation_type": "prior-session"}, {"identifier": "A 3101", "legislative_session": "2019-2020", "relation_type": "prior-session"}, {"identifier": "A 265