{"legislative_session": "2025-2026", "identifier": "A217", "title": "Relates to carport-mounted solar energy system siting", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Prohibits local governments from prohibiting the construction, installation, or operation of carport-mounted solar energy systems with a nameplate capacity of five megawatts or less, in any non-residential zoning district or mixed commercial and residential zoning district of any such city, town or village, provided however, if the zoning district is a certified agricultural district the agricultural impacts of the projects shall be minimized to the extent practicable."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO ENERGY", "date": "2025-01-08", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "ENERGY", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Chris Burdick", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Chris Burdick\"}", "organization_id": null}], "related_bills": [{"identifier": "S 6882", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "S 6882", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "A 9810", "legislative_session": "2023-2024", "relation_type": "prior-session"}], "versions": [{"note": "A217", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=printbill&bn=A217&term=2025&Text=Y", "media_type": "text/html"}, {"url": "http://legislation.nysenate.gov/pdf/bills/2025/A217", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "https://legislation.nysenate.gov/api/3/bills/2025-2026/A217?summary=&detail=", "note": ""}, {"url": "http://www.nysen