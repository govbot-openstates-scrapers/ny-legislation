{"legislative_session": "2025-2026", "identifier": "S999", "title": "Relates to reimbursement for early and periodic screening, diagnosis and treatment", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Requires the department of health to review claims for expenditures for early and periodic screening, diagnosis and treatment and other health services, care and supplies which are furnished to eligible children and pre-school children regardless of whether such children have handicapping conditions, are suspected of having handicapping conditions or have an individualized education plan; requires the department of health to apply for all necessary federal approvals regarding such expenditures."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO HEALTH", "date": "2025-01-08", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "HEALTH", "entity_type": "organization", "organization_id": null}]}, {"description": "REPORTED AND COMMITTED TO FINANCE", "date": "2025-04-29", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": []}], "sponsorships": [{"name": "Samra Brouk", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Samra Brouk\"}", "organization_id": null}, {"name": "Kristen Gonzalez", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Kristen Gonzalez\"}", "organization_id": null}, {"name": "Andrew Gounardes", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Andrew Gounardes\"}", "organization_id": null}, {"name": "Brad Hoylman-Sigal", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Brad Hoylman-Siga