{"legislative_session": "2025-2026", "identifier": "S1393", "title": "Relates to establishing a job creation tax credit", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Relates to establishing a job creation tax credit; provides that the amount of the credit shall be six percent of the total amount of wages paid to the qualified employee during the employee's first six months of employment and if the qualified employee was receiving unemployment insurance benefits at the time of hire for a minimum of thirteen weeks or is employed directly in the manufacturing process in an eligible industry, the amount of the credit shall be twelve percent of the total amount of wages paid to the qualified employee during the employee's first six months of employment; caps the credit at $750 for any qualified employee and $1500 for any qualified employee who was receiving unemployment insurance benefits at the time of hire for a minimum of thirteen weeks or who is employed directly in the manufacturing process in an eligible industry."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO INVESTIGATIONS AND GOVERNMENT OPERATIONS", "date": "2025-01-09", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "INVESTIGATIONS AND GOVERNMENT OPERATIONS", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Patricia Canzoneri-Fitzpatrick", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Patricia Canzoneri-Fitzpatrick\"}", "organization_id": null}], "related_bills": [{"identifier": "S 5977", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "S 1936", "legislative_session": "2021-2022", "relation_type": "prior-session"}, {"identifier": "S 541", "legislative_session": "2019-20