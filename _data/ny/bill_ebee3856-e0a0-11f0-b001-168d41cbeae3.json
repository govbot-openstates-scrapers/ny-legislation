{"legislative_session": "2025-2026", "identifier": "A2616", "title": "Provides for the posthumous awarding of high school diplomas", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Directs the board of education or the board of trustees of each school district shall establish a policy and adopt procedures permitting the district to confer a diploma on a student posthumously if, at the time of death, such student had completed the eleventh grade class of a public school in the district and was enrolled or expected to be enrolled in the twelfth grade class within the same public school district the following school year."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO EDUCATION", "date": "2025-01-21", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "EDUCATION", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Carrie Woerner", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Carrie Woerner\"}", "organization_id": null}, {"name": "Matthew Simpson", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Matthew Simpson\"}", "organization_id": null}, {"name": "Angelo Santabarbara", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Angelo Santabarbara\"}", "organization_id": null}, {"name": "Mary Beth Walsh", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Mary Beth Walsh\"}", "organization_id": null}, {"name": "John T. McDonald III", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"John T. McDonald III\"}", "organization_id": null}], "related_bills": [{"identifier": "S 831",