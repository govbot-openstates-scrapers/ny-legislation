{"legislative_session": "2025-2026", "identifier": "A6165", "title": "Enacts the wetlands homeowner's relief act", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Enacts the wetlands homeowner's relief act; provides that a homeowner who currently lives within a wetlands designated area shall not have to apply for a building permit for use of land within a fifty foot periphery of the house and garage; provides that new maps may not be issued without prior approval from the legislature; prohibits DEC from delineating wetlands beyond the boundaries shown on official wetlands maps."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO ENVIRONMENTAL CONSERVATION", "date": "2025-02-26", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "ENVIRONMENTAL CONSERVATION", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "John Lemondes", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"John Lemondes\"}", "organization_id": null}, {"name": "Paula Bologna", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Paula Bologna\"}", "organization_id": null}, {"name": "Stephen Hawley", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Stephen Hawley\"}", "organization_id": null}, {"name": "Josh Jensen", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Josh Jensen\"}", "organization_id": null}, {"name": "Joe DeStefano", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Joe DeStefano\"}", "organization_id": null}], "related_bills": [{"identifier": "A 4293", "legislative_session": "2023-2024", "relatio