{"legislative_session": "2025-2026", "identifier": "A8380", "title": "Requires the inspection of certain non-fireproof buildings and compliance with the New York city housing maintenance code and the New York city construction codes", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Requires the inspection of non-fireproof buildings in a city having a population of one million or more every three years; requires compliance with the New York city housing maintenance code and the New York city construction codes; requires those dwellings not in compliance with such code at the time of inspection to make repairs or improvements necessary to comply with such standards within six months of such inspection, or a lien shall be placed upon the dwelling."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO HOUSING", "date": "2025-05-13", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "HOUSING", "entity_type": "organization", "organization_id": null}]}, {"description": "AMEND AND RECOMMIT TO HOUSING", "date": "2025-06-04", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["amendment-passage", "referral-committee"], "related_entities": []}, {"description": "PRINT NUMBER 8380A", "date": "2025-06-04", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["amendment-passage"], "related_entities": []}], "sponsorships": [{"name": "Micah Lasher", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Micah Lasher\"}", "organization_id": null}], "related_bills": [{"identifier": "S 6600", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "S 6600", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "A 5415", "legislative_session": "2