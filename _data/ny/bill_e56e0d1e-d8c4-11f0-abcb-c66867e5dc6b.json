{"legislative_session": "2025-2026", "identifier": "A625", "title": "Relates to pendency placements in a school district in New York city", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Provides that in a school district in a city having a population of one million or more, the parent or person in parental relation to a child shall be entitled to the presumption that the school identified in a settlement agreement is the student's operative placement for the purposes of pendency until there is an intervening, pendency-changing event."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO EDUCATION", "date": "2025-01-08", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "EDUCATION", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Simcha Eichenstein", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Simcha Eichenstein\"}", "organization_id": null}, {"name": "William Colton", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"William Colton\"}", "organization_id": null}, {"name": "Stacey Pheffer Amato", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Stacey Pheffer Amato\"}", "organization_id": null}, {"name": "Robert C. Carroll", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Robert C. Carroll\"}", "organization_id": null}], "related_bills": [{"identifier": "A 2030", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "A 6576", "legislative_session": "2021-2022", "relation_type": "prior-session"}], "versions": [{"note": "A625", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=print