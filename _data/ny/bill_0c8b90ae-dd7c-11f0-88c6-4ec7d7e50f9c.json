{"legislative_session": "2025-2026", "identifier": "A7272", "title": "Requires the installation and periodic inspection of water detector alarms in residential dwellings and certain commercial buildings", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Requires the installation and periodic inspection of water detector alarms in residential dwellings and certain commercial buildings; defines a \"water detector alarm\" as an electronic device that detects the presence of water and sets off an audible and visual alarm when its sensors are triggered; defines a \"commercial building\" as a building in which individuals are employed."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO GOVERNMENTAL OPERATIONS", "date": "2025-03-21", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "GOVERNMENTAL OPERATIONS", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Monique Chandler-Waterman", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Monique Chandler-Waterman\"}", "organization_id": null}], "related_bills": [{"identifier": "A 5723", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "A 8601", "legislative_session": "2021-2022", "relation_type": "prior-session"}], "versions": [{"note": "A7272", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=printbill&bn=A7272&term=2025&Text=Y", "media_type": "text/html"}, {"url": "http://legislation.nysenate.gov/pdf/bills/2025/A7272", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "https://legislation.nysenate.gov/api/3/bills/2025-2026/A7272?summary=&detail=", "note": ""}, {"url": "http://www.nysenate.gov/legislation/bills/2025/A7272