{"legislative_session": "2025-2026", "identifier": "S6406", "title": "Allows beneficiaries of certain deceased members to elect to receive death benefits in a lump sum", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Allows a beneficiary of a member whose death occurs on or after July 1, 2025 and who would have been entitled to a service credit at the time of such member's death to elect to receive a lump sum payment equal to the pension reserve that would have been established had the member retired on the date of such member's death."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO CIVIL SERVICE AND PENSIONS", "date": "2025-03-13", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "CIVIL SERVICE AND PENSIONS", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Robert Jackson", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Robert Jackson\"}", "organization_id": null}, {"name": "Pete Harckham", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Pete Harckham\"}", "organization_id": null}, {"name": "Mark Walczyk", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Mark Walczyk\"}", "organization_id": null}], "related_bills": [{"identifier": "S 5849", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "S 8408", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "S 8400", "legislative_session": "2021-2022", "relation_type": "prior-session"}], "versions": [{"note": "S6406", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=printbill&bn=S6406&term=2025&Text=Y", "media_type": "text/html"}, {"url": "http: