{"legislative_session": "2025-2026", "identifier": "S4247", "title": "Provides Medicaid reimbursement for interpretation services provided by hospital inpatient and outpatient departments", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Provides Medicaid reimbursement for interpretation services provided by hospital inpatient, outpatient and emergency department settings and in diagnostic treatment centers; provides further that time frames for notice, approval or certification of rates of payment and to the requirement of prior notice of rates of payment are suspended, deemed without any force and effect from and after February 1, 2027 for such rates effective for the period April 1, 2027 through March 31, 2028."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO HEALTH", "date": "2025-02-03", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "HEALTH", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Kevin S. Parker", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Kevin S. Parker\"}", "organization_id": null}], "related_bills": [{"identifier": "S 6377", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "S 5073", "legislative_session": "2021-2022", "relation_type": "prior-session"}, {"identifier": "S 4615", "legislative_session": "2019-2020", "relation_type": "prior-session"}, {"identifier": "S 3419", "legislative_session": "2017-2018", "relation_type": "prior-session"}, {"identifier": "S 3974", "legislative_session": "2015-2016", "relation_type": "prior-session"}, {"identifier": "S 3188", "legislative_session": "2013-2014", "relation_type": "prior-session"}, {"identifier": "S 1138", "legislative_session": "2011-2012", "relation_type": "p