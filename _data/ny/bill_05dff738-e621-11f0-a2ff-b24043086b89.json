{"legislative_session": "2025-2026", "identifier": "S828", "title": "Relates to the New York state health care quality and cost containment commission", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Provides that the New York state health care quality and cost containment commission shall: evaluate each mandated benefit; investigate current practices of health plans with regard to the mandated benefit; investigate the potential premium impact of repealing and/or modifying the mandated benefits on all segments of the insurance market; hold at least two public hearings; submit a report to the legislature; makes related provisions."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO INSURANCE", "date": "2025-01-08", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "INSURANCE", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Patrick M. Gallivan", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Patrick M. Gallivan\"}", "organization_id": null}, {"name": "George Borrello", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"George Borrello\"}", "organization_id": null}], "related_bills": [{"identifier": "A 4498", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "A 4498", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "S 926", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "S 1720", "legislative_session": "2021-2022", "relation_type": "prior-session"}, {"identifier": "S 537", "legislative_session": "2019-2020", "relation_type": "prior-session"}, {"identifier": "S 1668", "legislative_session": "2017-2018", "relation_type": "p