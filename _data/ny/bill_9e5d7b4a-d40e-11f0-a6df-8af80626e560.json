{"legislative_session": "2025-2026", "identifier": "A8467", "title": "Relates to notice to fire departments when oxygen is in use at a residence", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Requires a medical equipment service agency that has supplied oxygen or an oxygen delivery system to a patient to notify the appropriate fire department or company serving the municipality in which the patient resides of the name, address, and age of the patient and the existence of the oxygen or oxygen delivery system at the patient's residence; provides for informed consent; requires notice to the fire department of removal of such equipment."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO HEALTH", "date": "2025-05-16", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "HEALTH", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Patrick J. Carroll", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Patrick J. Carroll\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "A8467", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=printbill&bn=A8467&term=2025&Text=Y", "media_type": "text/html"}, {"url": "http://legislation.nysenate.gov/pdf/bills/2025/A8467", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [{"publication": "New York Laws", "citation": "PBH \u00a7 3656", "citation_type": "proposed", "effective": null, "expires": null, "url": "https://www.nysenate.gov/legislation/laws/PBH/"}, {"publication": "New York Laws", "citation": "PBH \u00a7 3656", "citation_type": "proposed", "effective": null, "expires": null, "url": "https://www.nysenate.gov/legislation/laws/PBH/"}], "sources": [{"url": "https://legis