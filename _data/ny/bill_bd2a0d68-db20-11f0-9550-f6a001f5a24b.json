{"legislative_session": "2025-2026", "identifier": "A162", "title": "Relates to the provision of legal representation to certain persons in eviction, ejectment and foreclosure proceedings", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Requires counties and/or cities to establish a plan for providing legal counsel to persons who are defendants or respondents in eviction, ejectment and foreclosure proceedings and who are financially unable to obtain counsel; defines eligible person as one whose gross individual income is not in excess of one hundred twenty-five percent of the federal income official poverty line; requires the state to match dollar for dollar the amount counties appropriate for their plans."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO JUDICIARY", "date": "2025-01-08", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "JUDICIARY", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Linda Rosenthal", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Linda Rosenthal\"}", "organization_id": null}, {"name": "Steven Raga", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Steven Raga\"}", "organization_id": null}, {"name": "Chris Burdick", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Chris Burdick\"}", "organization_id": null}, {"name": "Marcela Mitaynes", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Marcela Mitaynes\"}", "organization_id": null}], "related_bills": [{"identifier": "S 7436", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "S 7436", "legislative_sessio