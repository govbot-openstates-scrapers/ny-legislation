{"legislative_session": "2025-2026", "identifier": "A1168", "title": "Directs the department of state to establish a statewide registry of mental hygiene and child supervision residential facilities", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Directs the department of state to establish and maintain a statewide registry of mental hygiene and child supervision residential facilities which shall include residential facilities for juvenile delinquents and persons in need of supervision subject to the supervision of the office of children and family services, for the mentally ill subject to the supervision of the office of mental health, for the developmentally disabled subject to the supervision of the office for people with developmental disabilities, and for the chemically dependent subject to the supervision of the office of alcoholism and substance abuse services."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO GOVERNMENTAL OPERATIONS", "date": "2025-01-09", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "GOVERNMENTAL OPERATIONS", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Philip Ramos", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Philip Ramos\"}", "organization_id": null}], "related_bills": [{"identifier": "A 4489", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "A 5602", "legislative_session": "2021-2022", "relation_type": "prior-session"}, {"identifier": "A 5225", "legislative_session": "2019-2020", "relation_type": "prior-session"}, {"identifier": "A 4549", "legislative_session": "2017-2018", "relation_type": "prior-session"}, {"identifier": "A 4431", "legislative_session": "2015-2016", "relation_type": "prior