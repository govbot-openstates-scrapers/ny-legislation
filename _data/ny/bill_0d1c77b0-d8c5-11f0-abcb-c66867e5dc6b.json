{"legislative_session": "2025-2026", "identifier": "S6401", "title": "Relates to death benefits for members of the uniformed force of the New York city department of sanitation and members of the uniformed force of the New York city department of correction", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Relates to death benefits for members of the uniformed force of the New York city department of sanitation and members of the uniformed force of the New York city department of correction; establishes that the beneficiaries of a member who would have been entitled to a service retirement benefit at the time of such member's death may elect to receive, in a lump sum, an amount payable which shall be equal to the pension reserve that would have been established had the member retired on the date of such member's death, or the value of the death benefit and the reserve-for-increased-take-home-pay, if any, whichever is greater."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO CIVIL SERVICE AND PENSIONS", "date": "2025-03-13", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "CIVIL SERVICE AND PENSIONS", "entity_type": "organization", "organization_id": null}]}, {"description": "REPORTED AND COMMITTED TO FINANCE", "date": "2025-04-08", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": []}], "sponsorships": [{"name": "Robert Jackson", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Robert Jackson\"}", "organization_id": null}], "related_bills": [{"identifier": "A 7335", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "A 7335", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "S 630