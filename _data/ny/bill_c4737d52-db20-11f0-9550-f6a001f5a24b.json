{"legislative_session": "2025-2026", "identifier": "A1036", "title": "Relates to referrals for child support enforcement for foster care maintenance payments", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Relates to liability to support a child who is placed in foster care pursuant to an approved application for foster care maintenance; provides that such liability shall not be imposed if it will adversely affect the health, safety or welfare of the child on whose behalf such payments are to be made or other persons in the child's household or will adversely affect the length of the child's placement or impair the ability of the child to return home when discharged from foster care; limits the ability of a county or local social services district to cause a referral to be sent for the purpose of commencing collection of child support for a child placed in foster care."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO JUDICIARY", "date": "2025-01-08", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "JUDICIARY", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Ron Kim", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Ron Kim\"}", "organization_id": null}, {"name": "Jaime R. Williams", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Jaime R. Williams\"}", "organization_id": null}, {"name": "Scott H. Bendett", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Scott H. Bendett\"}", "organization_id": null}, {"name": "Andrew Hevesi", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Andrew Hevesi\"}", "organization_id":