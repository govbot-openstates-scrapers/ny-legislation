{"legislative_session": "2025-2026", "identifier": "S3447", "title": "Requires state and county incarcerated individuals to make medical co-payments", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Requires a person incarcerated in state and county-owned or operated correctional facilities to make medical co-payments of seven dollars upon receipt of medical treatment; provides that an incarcerated individual shall not be refused treatment for lack of ability to pay co-payment charges; directs all moneys collected to be made available for the operation of such correctional facility."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO CRIME VICTIMS, CRIME AND CORRECTION", "date": "2025-01-27", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "CRIME VICTIMS, CRIME AND CORRECTION", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Pamela Helming", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Pamela Helming\"}", "organization_id": null}], "related_bills": [{"identifier": "A 4529", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "A 4529", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "S 2939", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "S 2572", "legislative_session": "2021-2022", "relation_type": "prior-session"}, {"identifier": "S 3128", "legislative_session": "2019-2020", "relation_type": "prior-session"}, {"identifier": "S 2580", "legislative_session": "2017-2018", "relation_type": "prior-session"}, {"identifier": "S 1686", "legislative_session": "2015-2016", "relation_type": "prior-session"}, {"identifier": "S 1894", "legislative_session": "2013-2014", "relation