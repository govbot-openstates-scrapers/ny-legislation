{"legislative_session": "2025-2026", "identifier": "S6600", "title": "Requires the inspection of certain non-fireproof buildings and compliance with the New York city housing maintenance code and the New York city construction codes", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Requires the inspection of non-fireproof buildings in a city having a population of one million or more every three years; requires compliance with the New York city housing maintenance code and the New York city construction codes; requires those dwellings not in compliance with such code at the time of inspection to make repairs or improvements necessary to comply with such standards within six months of such inspection, or a lien shall be placed upon the dwelling."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO HOUSING, CONSTRUCTION AND COMMUNITY DEVELOPMENT", "date": "2025-03-18", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "HOUSING, CONSTRUCTION AND COMMUNITY DEVELOPMENT", "entity_type": "organization", "organization_id": null}]}, {"description": "1ST REPORT CAL.627", "date": "2025-04-07", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}, {"description": "2ND REPORT CAL.", "date": "2025-04-08", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}, {"description": "ADVANCED TO THIRD READING", "date": "2025-04-09", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}, {"description": "AMENDED ON THIRD READING (T) 6600A", "date": "2025-04-15", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["amendment-passage"], "related_entities": []}, {"description": "AMENDED ON THIRD READING 6600B", "date": "2025-06-