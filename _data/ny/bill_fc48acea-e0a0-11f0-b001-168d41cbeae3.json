{"legislative_session": "2025-2026", "identifier": "A5374", "title": "Establishes a wound care kit and xylazine test strip distribution", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Establishes a wound care kit and xylazine test strip distribution program to supply wound care kits designed for the treatment of injection-related wounds and skin and soft tissue infections, and xylazine test strips, to individuals with a substance use disorder or who are otherwise at risk of drug use, free of charge; prohibits facilities from collecting personally identifying information on such individuals; prohibits police from targeting individuals receiving wound care kits or xylazine test strips under such program."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO ALCOHOLISM AND DRUG ABUSE", "date": "2025-02-13", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "ALCOHOLISM AND DRUG ABUSE", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Yudelka Tapia", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Yudelka Tapia\"}", "organization_id": null}, {"name": "Jessica Gonzalez-Rojas", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Jessica Gonzalez-Rojas\"}", "organization_id": null}, {"name": "Jen Lunsford", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Jen Lunsford\"}", "organization_id": null}], "related_bills": [{"identifier": "S 2446", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "S 2446", "legislative_session": "2025-2026", "relation_type": "companion"}], "versions": [{"note": "A5374", "links": [{"url": "http://assembly.state.ny.us/leg/?sh