{"legislative_session": "2025-2026", "identifier": "S952", "title": "Makes rent stabilized dwelling units eligible for certain protections", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Requires that the entire amount of a rental deposit or advance be refundable to the tenant upon the tenant's vacating of the premises except for an amount lawfully retained for non-payment of rent or utilities, damage caused by the tenant, or storage costs; requires landlord to provide an itemized statement of damages and to return the balance of the deposit within 14 days; requires that tenant be given the opportunity to inspect the rental premises prior to signing a lease; allows a grantee or assignee of the leased premises to demand that an escrow account be opened."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO JUDICIARY", "date": "2025-01-08", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "JUDICIARY", "entity_type": "organization", "organization_id": null}]}, {"description": "1ST REPORT CAL.344", "date": "2025-02-11", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}, {"description": "2ND REPORT CAL.", "date": "2025-02-12", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}, {"description": "ADVANCED TO THIRD READING", "date": "2025-02-24", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}, {"description": "AMENDED ON THIRD READING (T) 952A", "date": "2025-02-26", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["amendment-passage"], "related_entities": []}, {"description": "AMENDED ON THIRD READING 952B", "date": "2025-04-09", "organization_id": "~{\"classification\": \"upper\"}", "classif