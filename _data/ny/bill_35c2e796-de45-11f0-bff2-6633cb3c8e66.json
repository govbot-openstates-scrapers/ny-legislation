{"legislative_session": "2025-2026", "identifier": "A5624", "title": "Enacts the mental health assessment and record keeping for the coroner's office act", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Enacts the mental health assessment and record keeping for the coroner's office act; requires the coroner, coroner and coroner's physician, or the medical examiner, to conduct a mental health assessment when a death occurs in such person's jurisdiction; requires death certificates list an underlying mental illness as the cause of death when a person commits suicide; requires life insurance policies to pay upon a suicide which the coroner, coroner and coroner's physician, or the medical examiner indicated an underlying mental illness as the cause of death."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO LOCAL GOVERNMENTS", "date": "2025-02-18", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "LOCAL GOVERNMENTS", "entity_type": "organization", "organization_id": null}]}, {"description": "AMEND (T) AND RECOMMIT TO LOCAL GOVERNMENTS", "date": "2025-05-06", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["amendment-passage", "referral-committee"], "related_entities": []}, {"description": "PRINT NUMBER 5624A", "date": "2025-05-06", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["amendment-passage"], "related_entities": []}], "sponsorships": [{"name": "Nikki Lucas", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Nikki Lucas\"}", "organization_id": null}], "related_bills": [{"identifier": "A 9027", "legislative_session": "2023-2024", "relation_type": "prior-session"}], "versions": [{"note": "A5624", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=printbil