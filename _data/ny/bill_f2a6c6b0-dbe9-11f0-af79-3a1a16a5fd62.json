{"legislative_session": "2025-2026", "identifier": "S724", "title": "Relates to ADA or handicapped parking spaces at an arena", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Requires all ADA or handicapped parking spaces at an arena, entertainment or sports venue shall be directly outside the entrance door to the building as stated in federal law; provides that failure to comply with the provisions of this section shall result in an automatic suspension of any New York liquor license held by such venue for a period of one week for each day in which such venue is noncompliant; makes related provisions."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO TRANSPORTATION", "date": "2025-01-08", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "TRANSPORTATION", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Steve Rhoads", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Steve Rhoads\"}", "organization_id": null}], "related_bills": [{"identifier": "S 9328", "legislative_session": "2023-2024", "relation_type": "prior-session"}], "versions": [{"note": "S724", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=printbill&bn=S724&term=2025&Text=Y", "media_type": "text/html"}, {"url": "http://legislation.nysenate.gov/pdf/bills/2025/S724", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [{"publication": "New York Laws", "citation": "VAT \u00a7 1203-G-1", "citation_type": "proposed", "effective": null, "expires": null, "url": "https://www.nysenate.gov/legislation/laws/VAT/"}], "sources": [{"url": "https://legislation.nysenate.gov/api/3/bills/2025-2026/S724?summary=&detail=", "note": ""}, {"url": "http://www.nysenate.gov/legislati