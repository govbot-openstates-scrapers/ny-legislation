{"legislative_session": "2025-2026", "identifier": "A603", "title": "Requires electronic cigarettes and e-liquids to have nicotine levels which automatically taper off", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Requires the department of health to establish nicotine levels for electronic cigarettes and e-liquids which automatically taper in nicotine strength in amounts and at certain time intervals; requires manufacturers to only manufacture, cause to be manufactured, or sold, in this state, any electronic cigarette or e-liquid unless such product automatically tapers in nicotine strength in amounts and at certain time intervals as determined by the department of health."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO HEALTH", "date": "2025-01-08", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "HEALTH", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Linda Rosenthal", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Linda Rosenthal\"}", "organization_id": null}], "related_bills": [{"identifier": "A 143", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "A 646", "legislative_session": "2021-2022", "relation_type": "prior-session"}, {"identifier": "A 8629", "legislative_session": "2019-2020", "relation_type": "prior-session"}], "versions": [{"note": "A603", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=printbill&bn=A603&term=2025&Text=Y", "media_type": "text/html"}, {"url": "http://legislation.nysenate.gov/pdf/bills/2025/A603", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "https://legislation.nysenate.gov/api/3/bills/2025-2026/A603?summary