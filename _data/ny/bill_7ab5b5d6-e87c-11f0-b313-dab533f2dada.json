{"legislative_session": "2025-2026", "identifier": "A7335", "title": "Relates to death benefits for members of the uniformed force of the New York city department of sanitation and members of the uniformed force of the New York city department of correction", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Relates to death benefits for members of the uniformed force of the New York city department of sanitation and members of the uniformed force of the New York city department of correction; establishes that the beneficiaries of a member who would have been entitled to a service retirement benefit at the time of such member's death may elect to receive, in a lump sum, an amount payable which shall be equal to the pension reserve that would have been established had the member retired on the date of such member's death, or the value of the death benefit and the reserve-for-increased-take-home-pay, if any, whichever is greater."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO GOVERNMENTAL EMPLOYEES", "date": "2025-03-25", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "GOVERNMENTAL EMPLOYEES", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Stacey Pheffer Amato", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Stacey Pheffer Amato\"}", "organization_id": null}], "related_bills": [{"identifier": "S 6401", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "S 6401", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "A 6578", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "A 7335", "legislative_session": "2021-2022", "relation_type": "prior-session"}, {"identifier": "A 1116