{"legislative_session": "2025-2026", "identifier": "A8242", "title": "Renders candidates ineligible to receive public matching funds when such candidate has been found in a prior election cycle to have misused public campaign funds", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Renders candidates ineligible to receive public matching funds when such candidate has been found in a prior election cycle to have knowingly and willfully misused public campaign funds or committed fraud, conversion or unauthorized personal use of such funds; states that such ineligibility continues until the candidate has repaid such funds, satisfied civil penalties, and complied with orders imposed as a result of such prior misuse of public funds."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO ELECTION LAW", "date": "2025-05-05", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "ELECTION LAW", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Larinda Hooks", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Larinda Hooks\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "A8242", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=printbill&bn=A8242&term=2025&Text=Y", "media_type": "text/html"}, {"url": "http://legislation.nysenate.gov/pdf/bills/2025/A8242", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "https://legislation.nysenate.gov/api/3/bills/2025-2026/A8242?summary=&detail=", "note": ""}, {"url": "http://www.nysenate.gov/legislation/bills/2025/A8242", "note": ""}, {"url": "https://nyassembly.gov/leg/?bn=A08242&term=2025", "note": ""}], "extras": {}, "jurisdiction": {"id": "ocd-juris