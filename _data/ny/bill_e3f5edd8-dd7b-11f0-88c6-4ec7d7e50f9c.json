{"legislative_session": "2025-2026", "identifier": "A1011", "title": "Requires hospitals which offer medical residency training to provide certain obstetric-gynecology training", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Provides that hospitals which offer medical residency training in obstetrics, gynecology, internal medicine, women's health and osteopathy shall provide training which follows the Accreditation Council on Graduate Medical Education or the American Osteopathic Association special requirements for obstetrics-gynecology; provides for experience with induced abortions and complications thereto except where residency program has a religious, moral or legal restriction which prohibits residents from performing abortions within the institution."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO HEALTH", "date": "2025-01-08", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "HEALTH", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Deborah Glick", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Deborah Glick\"}", "organization_id": null}, {"name": "Karines Reyes", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Karines Reyes\"}", "organization_id": null}, {"name": "Harvey Epstein", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Harvey Epstein\"}", "organization_id": null}, {"name": "Chantel Jackson", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Chantel Jackson\"}", "organization_id": null}], "related_bills": [{"identifier": "S 3987", "legislative_session": "2025-2026", "relation_type": "companion