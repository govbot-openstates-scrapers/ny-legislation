{"legislative_session": "2025-2026", "identifier": "A8733", "title": "Relates to financial literacy instruction", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Directs the state board of regents to develop or adopt financial literacy learning standards for students in grades four through eight; provides that all public, charter, and nonpublic high schools shall offer and students at such schools shall successfully complete a course in financial literacy; directs the trustees of the state university of New York, in consultation with faculty, student representatives and subject matter experts, to develop a comprehensive financial literacy course; directs the trustees of the city university of New York, in consultation with faculty, student representatives and subject matter experts, to develop a comprehensive financial literacy course."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO EDUCATION", "date": "2025-06-02", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "EDUCATION", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Robert Smullen", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Robert Smullen\"}", "organization_id": null}, {"name": "Karl Brabenec", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Karl Brabenec\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "A8733", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=printbill&bn=A8733&term=2025&Text=Y", "media_type": "text/html"}, {"url": "http://legislation.nysenate.gov/pdf/bills/2025/A8733", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [{"publication": "New York Laws", "