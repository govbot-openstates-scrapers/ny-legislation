{"legislative_session": "2025-2026", "identifier": "A1391", "title": "Relates to the reporting of animal cruelty", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Provides that any person charged with enforcing laws prohibiting cruelty to animals shall cause a report to be made when, in the performance of their duties, such person has reasonable cause to believe that abuse or maltreatment of a child has also occurred; provides that a person charged with the responsibility of filing a report of child abuse or maltreatment shall also file a report of suspected animal cruelty."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO CHILDREN AND FAMILIES", "date": "2025-01-09", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "CHILDREN AND FAMILIES", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Deborah Glick", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Deborah Glick\"}", "organization_id": null}, {"name": "Jo Anne Simon", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Jo Anne Simon\"}", "organization_id": null}, {"name": "Vivian Cook", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Vivian Cook\"}", "organization_id": null}, {"name": "Christopher Eachus", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Christopher Eachus\"}", "organization_id": null}], "related_bills": [{"identifier": "S 5544", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "S 5544", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "A 3908", "legislative_session": "2023-2024", "re