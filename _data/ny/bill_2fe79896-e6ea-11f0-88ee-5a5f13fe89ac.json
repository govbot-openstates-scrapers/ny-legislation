{"legislative_session": "2025-2026", "identifier": "A891", "title": "Confinement of certain animals for food producing purposes", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Pertains to confinement of certain animals for food producing purposes; prohibits any person to tether or confine any pig during pregnancy or calf raised for veal for all or the majority of any day in a manner that prevents such animal from lying down, standing up and fully extending its limbs and turning around freely; establishes that commission of such crime shall constitute a class A misdemeanor punishable by imprisonment for a period not to exceed one year and/or fine not to exceed $1,000."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO AGRICULTURE", "date": "2025-01-08", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "AGRICULTURE", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Linda Rosenthal", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Linda Rosenthal\"}", "organization_id": null}, {"name": "David Weprin", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"David Weprin\"}", "organization_id": null}, {"name": "William Colton", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"William Colton\"}", "organization_id": null}], "related_bills": [{"identifier": "A 473", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "A 2122", "legislative_session": "2021-2022", "relation_type": "prior-session"}, {"identifier": "A 800", "legislative_session": "2019-2020", "relation_type": "prior-session"}, {"identifier": "A 1147", "legislative_session": "2017-2018",