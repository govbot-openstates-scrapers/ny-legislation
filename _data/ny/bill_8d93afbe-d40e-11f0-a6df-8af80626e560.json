{"legislative_session": "2025-2026", "identifier": "A6423", "title": "Makes rent stabilized dwelling units eligible for certain protections", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Requires that the entire amount of a rental deposit or advance be refundable to the tenant upon the tenant's vacating of the premises except for an amount lawfully retained for non-payment of rent or utilities, damage caused by the tenant, or storage costs; requires landlord to provide an itemized statement of damages and to return the balance of the deposit within 14 days; requires that tenant be given the opportunity to inspect the rental premises prior to signing a lease; allows a grantee or assignee of the leased premises to demand that an escrow account be opened."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO JUDICIARY", "date": "2025-03-04", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "JUDICIARY", "entity_type": "organization", "organization_id": null}]}, {"description": "AMEND AND RECOMMIT TO JUDICIARY", "date": "2025-04-08", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["amendment-passage", "referral-committee"], "related_entities": []}, {"description": "PRINT NUMBER 6423A", "date": "2025-04-08", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["amendment-passage"], "related_entities": []}, {"description": "REPORTED REFERRED TO CODES", "date": "2025-04-15", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "CODES", "entity_type": "organization", "organization_id": null}]}, {"description": "REPORTED REFERRED TO RULES", "date": "2025-04-28", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committ