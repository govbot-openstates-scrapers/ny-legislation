{"legislative_session": "2025-2026", "identifier": "S4587", "title": "Relates to prescription labels for mifepristone and misoprostol", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Provides the prescription label for mifepristone or misoprostol may include the address of the dispensing health care practice and shall include the address of the prescribing health care practice at the prescriber's request; provides that a health care prescriber shall inform a patient if such prescriber has included or has requested to include the name or address of the prescribing health care practice instead of the name of such prescriber on the prescription label when prescribing mifepristone or misoprostol."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO RULES", "date": "2025-02-07", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "RULES", "entity_type": "organization", "organization_id": null}]}, {"description": "ORDERED TO THIRD READING CAL.447", "date": "2025-03-04", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}, {"description": "SUBSTITUTED BY A5285", "date": "2025-03-10", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Shelley Mayer", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Shelley Mayer\"}", "organization_id": null}, {"name": "Lea Webb", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Lea Webb\"}", "organization_id": null}], "related_bills": [{"identifier": "A 5285", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "A 5285", "legislative_session": "2025-2026", "relation_type": "companion