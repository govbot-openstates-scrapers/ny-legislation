{"legislative_session": "2025-2026", "identifier": "S7778", "title": "Requires commissioners of local social services districts to screen, apply for, and use and conserve federal benefits on behalf of children placed in foster care", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Requires commissioners of local social services districts to screen, apply for, and use and conserve retirement, survivors and disability insurance, supplemental security income, veterans' or any other federal social security benefits on behalf of children placed in foster care; provides for responsibilities of local commissioners of social services appointed to serve as the representative payee for a child; provides for technical assistance and financial literacy for foster children; makes related provisions."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO CHILDREN AND FAMILIES", "date": "2025-05-06", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "CHILDREN AND FAMILIES", "entity_type": "organization", "organization_id": null}]}, {"description": "REPORTED AND COMMITTED TO FINANCE", "date": "2025-05-29", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": []}], "sponsorships": [{"name": "Jabari Brisport", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Jabari Brisport\"}", "organization_id": null}], "related_bills": [{"identifier": "A 8036", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "A 8036", "legislative_session": "2025-2026", "relation_type": "companion"}], "versions": [{"note": "S7778", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=printbill&bn=S7778&term=2025&Text=Y", "media_type": "text/html"}, {"url": "http://le