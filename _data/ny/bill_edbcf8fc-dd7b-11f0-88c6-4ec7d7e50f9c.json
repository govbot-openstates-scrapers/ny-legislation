{"legislative_session": "2025-2026", "identifier": "A2405", "title": "Enacts the New York state product promotion act", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Enacts the New York state product promotion act; requires the commissioner of economic development to develop and implement the New York state product promotion program; further requires such commissioner to establish and maintain an internet registry to highlight businesses that manufacture or produce products in this state; allows the department of economic development to charge a reasonable fee to cover such administrative costs; requires the commissioner to create a product label with the registry address to help identify such products."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO ECONOMIC DEVELOPMENT", "date": "2025-01-16", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "ECONOMIC DEVELOPMENT", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Robert Smullen", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Robert Smullen\"}", "organization_id": null}, {"name": "Andrew Molitor", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Andrew Molitor\"}", "organization_id": null}, {"name": "Andrea Bailey", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Andrea Bailey\"}", "organization_id": null}], "related_bills": [{"identifier": "A 4690", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "A 2290", "legislative_session": "2021-2022", "relation_type": "prior-session"}, {"identifier": "A 4864", "legislative_session": "2019-2020", "relation_type": "prior-session"}, {"i