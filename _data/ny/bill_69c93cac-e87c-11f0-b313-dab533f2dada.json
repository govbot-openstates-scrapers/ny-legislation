{"legislative_session": "2025-2026", "identifier": "A5053", "title": "Increases the minimum insurance coverage required for motor vehicles registered and/or operated within the state of New York", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Increases the minimum insurance coverage required for motor vehicles registered and/or operated within the state of New York; for injury or death to one person in any one accident, increases the limits required from $25,000 to $50,000 for bodily injury and from $50,000 to $100,000 for death, for injury or death to more than one person in any one accident, increases the limits required from $50,000 to $100,000 for bodily injury and from $100,000 to $200,000 for death, and for third party property damage in any one accident, increases the limits required from $10,000 to $25,000."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO INSURANCE", "date": "2025-02-11", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "INSURANCE", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Jonathan Jacobson", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Jonathan Jacobson\"}", "organization_id": null}], "related_bills": [{"identifier": "A 948", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "A 717", "legislative_session": "2021-2022", "relation_type": "prior-session"}, {"identifier": "A 7979", "legislative_session": "2019-2020", "relation_type": "prior-session"}], "versions": [{"note": "A5053", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=printbill&bn=A5053&term=2025&Text=Y", "media_type": "text/html"}, {"url": "http://legislation.nysenate.gov/pdf/bills/2025/A5053", "media_type": "application/pdf"}], "da