{"legislative_session": "2025-2026", "identifier": "A1516", "title": "Provides for limitations on investments of public pension funds and state contracts", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Provides for limitations on investments of public pension funds and state contracts; prohibits the investment of the available monies of the common retirement fund in any stocks, securities, equities, assets or other obligations of any corporation or company, or any subsidiary, affiliate or parent of any corporation or company, engaged in the boycott of Israel, including Iran-restricted companies and Sudan-restricted companies; prohibits any firm, partnership or corporation that boycotts Israel, that is an Iran-restricted company or that is a Sudan-restricted company from contracting with the state."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO GOVERNMENTAL EMPLOYEES", "date": "2025-01-10", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "GOVERNMENTAL EMPLOYEES", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "David Weprin", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"David Weprin\"}", "organization_id": null}], "related_bills": [{"identifier": "A 881", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "A 8271", "legislative_session": "2021-2022", "relation_type": "prior-session"}], "versions": [{"note": "A1516", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=printbill&bn=A1516&term=2025&Text=Y", "media_type": "text/html"}, {"url": "http://legislation.nysenate.gov/pdf/bills/2025/A1516", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "https://l