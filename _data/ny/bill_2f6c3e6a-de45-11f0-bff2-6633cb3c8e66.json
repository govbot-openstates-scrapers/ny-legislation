{"legislative_session": "2025-2026", "identifier": "A4471", "title": "Enacts the \"Lead-Free Homes Act\"", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Enacts the \"Lead Free Homes Act\"; requires the department of health to promulgate standards for lead remediation and abatement of exemption; provides tax credits to class A multiple dwelling owners who undertake a successful lead remediation or abatement; requires lead testing on drinking water prior to the sale of residential property; requires owners of class A multiple dwellings to perform lead testing and provides for fines for violations."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO HEALTH", "date": "2025-02-04", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "HEALTH", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Brian Cunningham", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Brian Cunningham\"}", "organization_id": null}], "related_bills": [{"identifier": "A 5054", "legislative_session": "2023-2024", "relation_type": "prior-session"}, {"identifier": "S 6969", "legislative_session": "2021-2022", "relation_type": "prior-session"}], "versions": [{"note": "A4471", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=printbill&bn=A4471&term=2025&Text=Y", "media_type": "text/html"}, {"url": "http://legislation.nysenate.gov/pdf/bills/2025/A4471", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [{"publication": "New York Laws", "citation": "MDW \u00a7 85", "citation_type": "proposed", "effective": null, "expires": null, "url": "https://www.nysenate.gov/legislation/laws/MDW/"}, {"publication": "New York Laws", "citation": "RPP \u00a7 242-A", "citation_ty