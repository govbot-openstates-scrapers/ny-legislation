{"legislative_session": "2025-2026", "identifier": "A6960", "title": "Prohibits electric corporations and gas corporations from increasing charges due to tariffs imposed by the United States or governments of countries or their political subdivisions outside the United States", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Prohibits electric corporations, gas corporations and utilities from passing along costs or increasing charges to ratepayers for electricity and natural gas, or any associated goods and services, as a result of increased costs incurred by such corporation due to tariffs placed by the government of the United States on the importation of goods, services or materials from countries outside the United States or due to tariffs placed by the governments of countries or their political subdivisions on the exportation of goods, services, or materials to the United States."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO ENERGY", "date": "2025-03-18", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "ENERGY", "entity_type": "organization", "organization_id": null}]}, {"description": "AMEND (T) AND RECOMMIT TO ENERGY", "date": "2025-03-19", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["amendment-passage", "referral-committee"], "related_entities": []}, {"description": "PRINT NUMBER 6960A", "date": "2025-03-19", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["amendment-passage"], "related_entities": []}], "sponsorships": [{"name": "Jonathan Jacobson", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Jonathan Jacobson\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "A6960", "links": [{"url": "http://assembly.state.ny.u