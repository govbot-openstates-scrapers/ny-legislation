{"legislative_session": "2025-2026", "identifier": "S1497", "title": "Relates to the paper carryout bag reduction fee", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Implements a one year phase-in period for a local law, ordinance or resolution imposing a paper carryout bag reduction fee where forty percent of such fees are returned to such county or city for the purpose of purchasing and distributing reusable bags, with priority given to low- and fixed-income communities, and after the first year, such forty percent of fees shall be remitted to the person who collected the fees."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO ENVIRONMENTAL CONSERVATION", "date": "2025-01-10", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "ENVIRONMENTAL CONSERVATION", "entity_type": "organization", "organization_id": null}]}, {"description": "REPORTED AND COMMITTED TO FINANCE", "date": "2025-05-28", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": []}], "sponsorships": [{"name": "Patricia Fahy", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Patricia Fahy\"}", "organization_id": null}], "related_bills": [{"identifier": "A 5072", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "A 5072", "legislative_session": "2025-2026", "relation_type": "companion"}], "versions": [{"note": "S1497", "links": [{"url": "http://assembly.state.ny.us/leg/?sh=printbill&bn=S1497&term=2025&Text=Y", "media_type": "text/html"}, {"url": "http://legislation.nysenate.gov/pdf/bills/2025/S1497", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [{"publication": "New York Laws", "citation": "ENV \u00a