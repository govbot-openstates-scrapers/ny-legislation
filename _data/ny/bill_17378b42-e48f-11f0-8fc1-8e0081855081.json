{"legislative_session": "2025-2026", "identifier": "A7453", "title": "Enacts the \"New York City Training And Comprehension of Trauma in Children (TACTIC) Act\"", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Enacts the \"New York City Training And Comprehension of Trauma in Children (TACTIC) Act\"; establishes the TACTIC council; provides for a trauma informed care training program to teachers in schools located in the city of New York; creates a department of child trauma informed care to assist with parenting workshops, to assign trauma informed care specialists to schools and to report to the TACTIC council regarding child trauma informed care provided to schools in the city of New York."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "REFERRED TO CITIES", "date": "2025-03-28", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "CITIES", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Manny De Los Santos", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Manny De Los Santos\"}", "organization_id": null}, {"name": "Kwani O'Pharrow", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Kwani O'Pharrow\"}", "organization_id": null}, {"name": "Dana Levenberg", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Dana Levenberg\"}", "organization_id": null}, {"name": "Yudelka Tapia", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Yudelka Tapia\"}", "organization_id": null}], "related_bills": [{"identifier": "S 3983", "legislative_session": "2025-2026", "relation_type": "companion"}, {"identifier": "S 3983", "legislative_session": "202